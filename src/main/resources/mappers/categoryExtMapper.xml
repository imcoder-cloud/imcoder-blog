<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fun.imcoder.cloud.mapper.CategoryExtMapper">

    <select id="getByCategoryId" resultType="java.util.Map">
        select * from category_ext a where a.category_id = #{id}
    </select>

</mapper>
